        "sensor",
        "weather",
        "风速",
        "风向",
        "传感器",
        "气象",
        "雨量",
        "降雨"
    ],
    "scripts": {},
    "dependencies": {},
    "devDependencies": {},
    "tested": true,
    "tester": "openjumper"
}
