{
  "name": "@aily-project/lib-dfrobot_asr_m",
  "nickname": "DFRobot语音识别模块",
  "author": "Vonweller",
  "description": "DFRobot_ASR_M语音识别模块控制库，适用于Arduino、ESP32等开发板。使用I2C接口进行通信，支持循环模式、指令模式和按钮模式三种识别模式，可添加自定义语音词条并识别返回对应的识别号。",
  "version": "0.0.1",
  "compatibility": {
    "core": [
      "arduino:avr",
      "esp32:esp32",
      "esp32:esp32s3"
    ],
    "voltage": [
      3.3,
      5
    ]
  },
  "keywords": [
    "dfrobot_asr",
    "语音识别",
    "ASR",
    "speech",
    "recognition"
  ],
  "scripts": {},
  "dependencies": {},
  "devDependencies": {},
  "tested": false,
  "tester": "",
  "url": "https://github.com/DFRobot/DFRobot_ASR_M"
}
