    "args0": [
        {
            "options": [
                ["DHT11", "DHT11"],
                ["DHT22", "DHT22"],
                ["DHT21", "DHT21"]
            ]
        },
        {
            "options": "${board.digitalPins}"
        }
    ]
},
"dht_read_success": {
    "message0": "DHT %1 传感器 引脚 %2 读取成功",
    "args0": [
        {
            "options": [
                ["DHT11", "DHT11"],
                ["DHT22", "DHT22"],
                ["DHT21", "DHT21"]
