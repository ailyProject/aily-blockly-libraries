{
"toolbox_name": "DHT传感器",
"dht_init": {
    "message0": "初始化 DHT %1 传感器 连接到引脚 %2",
    "args0": [
        {
            "options": [
                ["DHT11", "DHT11"],
                ["DHT22", "DHT22"],
                ["DHT21", "DHT21"]
            ]
        },
        {
            "options": "${board.digitalPins}"
        }
    ]
},
"dht_read_temperature": {
    "message0": "从 DHT %1 传感器 引脚 %2 读取温度 (℃)",
    "args0": [
        {
            "options": [
                ["DHT11", "DHT11"],
                ["DHT22", "DHT22"],
                ["DHT21", "DHT21"]
            ]
        },
        {
            "options": "${board.digitalPins}"
        }
    ]
},
"dht_read_humidity": {
