{
    "toolbox_name": "DHT传感器",
    "dht_init": {
        "message0": "初始化 DHT %1 传感器 类型 %2 连接到引脚 %3",
        "args0": [
            {
                "text": "dht"
            },
            {
                "options": [
                    ["DHT11", "DHT11"],
                    ["DHT22", "DHT22"],
                    ["DHT21", "DHT21"]
                ]
            },
            {
                "options": "${board.digitalPins}"
            }
        ]
    },
    "dht_read_temperature": {
        "message0": "从 DHT %1 传感器 读取温度 (℃)",
        "args0": [
            {
                "variable": "dht",
                "variableTypes": ["DHT"],
                "defaultType": "DHT"
            }
        ]
    },
    "dht_read_humidity": {
        "message0": "从 DHT %1 传感器 读取湿度 (%)",
        "args0": [
            {
                "variable": "dht",
                "variableTypes": ["DHT"],
                "defaultType": "DHT"
            }
        ]
    },
    "dht_read_success": {
        "message0": "DHT %1 传感器 读取成功",
        "args0": [
            {
                "variable": "dht",
                "variableTypes": ["DHT"],
                "defaultType": "DHT"
            }
        ]
    }
}