{
    "name": "@aily-project/lib-servo360",
    "nickname": "360舵机驱动",
    "author": "aily Project",
    "description": "360舵机控制支持库，过PWM信号实现对360度连续旋转舵机的速度和方向控制，支持正反转、速度调节、停止控制，适用于Arduino UNO、MEGA、UNO R4等开发板",
    "version": "1.0.0",
    "compatibility": {
        "core": [
            "arduino:avr",
            "renesas_uno:minima",
            "renesas_uno:unor4wifi"
        ],
        "voltage": [
            3.3,
            5
        ]
    },
    "keywords": [
        "aily",
        "blockly",
        "servo",
        "servo_attach",
        "servo_write",
        "执行器"
    ],
    "scripts": {},
    "dependencies": {},
    "devDependencies": {},
    "tested": true,
    "tester": "mango-0616"
}