{
    "toolbox_name": "Tone",
    "io_tone": {
        "message0": "Play tone of frequency %2 on pin %1",
        "args0": [
            {
                "options": "${board.digitalPins}"
            },
            null
        ]
    },
    "io_tone_duration": {
        "message0": "Play tone of frequency %2 on pin %1 for %3 milliseconds",
        "args0": [
            {
                "options": "${board.digitalPins}"
            },
            null,
            null
        ]
    },
    "io_system_sound": {
        "message0": "Play system sound %2 on pin %1",
        "args0": [
            {
                "options": "${board.digitalPins}"
            },
            {
                "options": [
                    ["Startup", "startup"],
                    ["Success", "success"],
                    ["Error", "error"],
                    ["Warning", "warning"],
                    ["Notification", "notification"],
                    ["Beep", "beep"],
                    ["Doorbell", "doorbell"],
                    ["Alarm", "alarm"],
                    ["Coin", "coin"],
                    ["Powerdown", "powerdown"]
                ]
            }
        ]
    },
    "io_note": {
        "message0": "Note %1",
        "args0": [
            {
                "options": [
                    ["C4 (261Hz)", "261"],
                    ["C#4 (277Hz)", "277"],
                    ["D4 (294Hz)", "294"],
                    ["D#4 (311Hz)", "311"],
                    ["E4 (330Hz)", "330"],
                    ["F4 (349Hz)", "349"],
                    ["F#4 (370Hz)", "370"],
                    ["G4 (392Hz)", "392"],
                    ["G#4 (415Hz)", "415"],
                    ["A4 (440Hz)", "440"],
                    ["A#4 (466Hz)", "466"],
                    ["B4 (494Hz)", "494"],
                    ["C5 (523Hz)", "523"],
                    ["D5 (587Hz)", "587"],
                    ["E5 (659Hz)", "659"],
                    ["F5 (698Hz)", "698"],
                    ["G5 (784Hz)", "784"],
                    ["A5 (880Hz)", "880"],
                    ["B5 (988Hz)", "988"],
                    ["Rest", "0"]
                ]
            }
        ]
    },
    "io_music": {
        "message0": "Play music %2 on pin %1",
        "args0": [
            {
                "options": "${board.digitalPins}"
            },
            {
                "options": [
                    ["Twinkle Star", "twinkle"],
                    ["Happy Birthday", "birthday"],
                    ["Castle in the Sky", "castle"],
                    ["Mary Had a Little Lamb", "mary"],
                    ["Ode to Joy", "joy"],
                    ["Only Mother is Good", "mother"],
                    ["Little Bee", "bee"],
                    ["Two Tigers", "tiger"]
                ]
            }
        ]
    },
    "io_notone": {
        "message0": "Stop tone on pin %1",
        "args0": [
            {
                "options": "${board.digitalPins}"
            }
        ]
    }
}