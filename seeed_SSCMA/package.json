{
  "name": "@aily-project/lib-seeed-sscma",
  "nickname":"Seeed SSCMA AI",
  "version": "1.0.0",
  "description": "Seeed SSCMA AI视觉传感器通信库，通过I2C/UART/SPI协议读取Grove Vision AI V2、XIAO ESP32S3 Sense等AI视觉模组的推理结果。支持目标检测、图像分类、关键点检测、人脸识别、手势识别等多种预训练模型，可部署自定义AI模型，适用于多平台微控制器",
  "author": "SeeedStudio",
  "compatibility": {
    "core": [
      "esp32:esp32",
      "esp32:esp32c3",
      "esp32:esp32s2",
      "esp32:esp32s3",
      "esp32:esp32c6",
      "esp32:esp32h2",
      "arduino:samd",
      "renesas_uno:unor4wifi",
      "rp2040:rp2040"
    ],
    "voltage": [3.3, 5]
  },
