{
  "name": "@aily-project/lib-esp32-sd",  
  "nickname": "ESP32 SD卡库",     
  "author": "esp32",
  "description": "SD卡及文件系统操作库,通过SPI实现对SD卡的文件读写操作，简化大容量数据存储与管理，适配ESP32系列开发板。支持多种文件系统（如FAT32）、文件和目录操作、数据流读写、卡容量检测。", 
  "version": "0.0.1",
  "compatibility": {                  
    "core": [                       
      "esp32:esp32",
      "esp32:esp32s3"
    ],
    "voltage": [3.3, 5]            
  },
  "keywords": [                     
    "sd",
    "SD卡",
    "存储"
  ],
  "scripts": {},
  "dependencies": {},                
  "devDependencies": {},
  "tested": true,                    
  "tester": "openjumper"             
}